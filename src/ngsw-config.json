{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "versionedFiles": [
          "/*.bundle.css",
          "/*.bundle.js",
          "/*.chunk.js"
        ],
        "files": [
          "/favicon.ico",
          "/index.html"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icons/**",
          "/assets/img/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": [
        "https://jsonplaceholder.typicode.com/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 2000,
        "maxAge": "365d"
      }
    }
  ]
}
