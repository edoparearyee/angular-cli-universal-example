{
  "routing": {
    "index": "/index.html",
    "routes": {
      "/": {
        "prefix": false
      },
      "/about": {
        "prefix": false
      }
    }
  },
  "dynamic": {
    "group": [
      {
        "name": "post api",
        "urls": {
          "https://jsonplaceholder.typicode.com": {
            "match": "prefix"
          }
        },
        "cache": {
          "optimizeFor": "performance",
          "maxAgeMs": 3600000,
          "maxEntries": 20,
          "strategy": "lru"
        }
      }
    ]
  }
}
